{
  "name": "PreferColorScheme",
  "description": "Switch Curent Tab Your Prefer Color Scheme just in one click",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "debugger"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}